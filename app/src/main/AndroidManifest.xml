<?xml version="1.0" encoding="UTF-8"?>
    <manifest
    xmlns:tools="http://schemas.android.com/tools"
    android:targetSandboxVersion="2" package="br.com.digitalhouse.harvardartmuseums"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:ignore="GradleOverrides,UnusedAttribute">



<application
    android:theme="@style/Theme.AppCompat.Light.NoActionBar"
    android:testOnly="true"
    android:supportsRtl="true"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:label="@string/app_name"
    android:icon="@mipmap/ic_launcher"
    android:debuggable="true"
    android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
    android:allowBackup="true"
    tools:ignore="AllowBackup,ExtraText,GoogleAppIndexingWarning,HardcodedDebugMode">

    <activity
        android:name="br.com.digitalhouse.harvardartmuseums.game.GameControlsActivity"
        android:theme="@style/AppTheme"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.information.InformationActivity"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.settings.SettingActivity"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.home.Home"/>

    <activity
        android:theme="@style/NoActionBar"
        android:label="@string/title_activity_favs"
        android:name="br.com.digitalhouse.harvardartmuseums.favs.FavsActivity"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.events.EventsActivity"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.login.LoginActivity"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.main.MainActivity"/>

    <activity
        android:theme="@style/AppTheme.NoActionBar"
        android:label="@string/title_activity_menu"
        android:name="br.com.digitalhouse.harvardartmuseums.menu.MenuActivity"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.login.CadastroLoginActivity"/>


    <activity android:name="br.com.digitalhouse.harvardartmuseums.splash.SplashActivity">


    <intent-filter>

    <action android:name="android.intent.action.MAIN"/>

    <category android:name="android.intent.category.LAUNCHER"/>

</intent-filter>

</activity>

    <activity
        android:label="Lower Level"
        android:name="br.com.digitalhouse.harvardartmuseums.home.LowerLevelActivity"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.splash.SplashActivityLowerLevel"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.game.GameMainActivity"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.game.GamePlay1Activity"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.game.GamePlay2Activity"/>

    <activity android:name="br.com.digitalhouse.harvardartmuseums.help.HelpActivity"/>


</application>

</manifest>